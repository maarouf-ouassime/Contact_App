<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="contact">
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name="ma.enset.contact.MainActivity"
            android:exported="true"
            android:label="Contact App"
            android:theme="@style/Theme.AnnuaireDeContact.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ma.enset.contact.MainActivity2"
            android:exported="true"
            android:label="Ajouter Contact"
            android:theme="@style/Theme.AnnuaireDeContact.NoActionBar"
            />
        <activity android:name="ma.enset.contact.MainActivity3"
            android:exported="true"
            android:label="Modifier Contact"
            android:theme="@style/Theme.AnnuaireDeContact.NoActionBar"
            />
    </application>

</manifest>